mc.playerController.windowClick(0, garbageItem, 0, 0, mc.player)
mc.playerController.windowClick(0, -999, 0, 0, mc.player)

HashMap<EntityPlayer, renderBlock> list = new HashMap();

List<EntityPlayer> playerList = mc.world.playerEntities;
for (EntityPlayer player : list.keySet()) {
    if (!playerList.contains(player)) 
        list.remove(player);
    }
}
for (EntityPlayer player : playerList) {
    if (list.containsKey(player)) {
        list.get(player).render();
    }
}
